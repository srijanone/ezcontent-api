{
  "name": "srijanone/ezcontent_api",
  "description": "Enable api support and preview module",
  "keywords": ["ezcontent", "preview", "Drupal"],
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "homepage": "https://github.com/srijanone/ezcontent_api",
  "support": {
    "issues": "https://github.com/srijanone/ezcontent_api",
    "source": "https://github.com/srijanone/ezcontent_api"
  },
  "require": {
    "drupal/access_unpublished": "^1.0@alpha",
    "drupal/decoupled_router": "^1.11",
    "drupal/gatsby": "^1.0@beta",
    "drupal/openapi": "^2",
    "drupal/openapi_ui": "^1.0@RC",
    "drupal/openapi_ui_redoc": "^1.0@RC",
    "drupal/openapi_ui_swagger": "^1.0@RC",
    "drupal/jsonapi_boost": "^1.0"
  },
  "extra": {
    "patches": {
      "drupal/core": {
        "Expose Layout Builder data to REST and JSON:API": "https://www.drupal.org/files/issues/2020-05-20/2942975-100.patch"
      },
      "drupal/metatag": {
        "Support JSON API, REST, GraphQL and custom normalizations": "https://www.drupal.org/files/issues/2020-02-23/2945817-87.patch"
      }
    }
  }
}
